namespace: code

server:
  persistence:
    storageClass: openebs-hostpath
    size: 50Gi

agent:
  replicaCount: 2
  persistence:
    storageClass: openebs-hostpath
    size: 30Gi

postgresql:
  external: true
  host: "postgresql.code.svc.cluster.local"
  password: "{{ .Values.secrets.postgresPassword }}"

ingress:
  enabled: true
  host: teamcity.{{ .Values.infra.host }}.local

consul:
  enabled: true
  host: "consul.consul.svc.cluster.local"
